<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Visualization</display-name>
  <welcome-file-list>
    <welcome-file>visualization.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>YoutubeServlet</servlet-name>
    <servlet-class>com.ade.servlet.YoutubeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>YoutubeServlet</servlet-name>
    <url-pattern>/youtube</url-pattern>
  </servlet-mapping>
</web-app>